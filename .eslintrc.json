{
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },

  "env": {
    "es6": true,
    "node": true
  },

  "extends": [
    "standard",
    "plugin:vue/recommended"
  ],

  "plugins": [
    "import",
    "node",
    "promise",
    "standard",
    "vue"
  ]
}
